  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "backend": "uv run main.py",
    "dev": "concurrently \"bun run backend\" \"bun run dev:frontend\"",
    "dev:frontend": "next dev --turbopack",
